body.mainbody			{background-color:black;background-attachment:fixed;background-repeat:no-repeat;background-position:center center;color:white;margin-top:5}
img.mouse_out			{border-width:0;}
img.mouse_on			{border:1 inset yellow;cursor:hand;filter:glow(color=green,strength=10);}
td.head					{font-size:9pt;border-width:0;border-bottom:1 solid silver;}
select.list				{background-color:black;border:0 solid black;width:120;overflow:none}
div.music_list			{color:#BEF73C;font-size:10pt;margin:5 0;text-align:right;cursor:hand}
a.link_list				{color:#BEF73C;font-size:10pt;text-decoration:none}
a.link_list:hover		{color:white;font-size:10pt;text-decoration:underline}
a.old					{color:#BEF73C;}
a.old:hover				{color:white;text-decoration:none}
marquee.musicText		{font-size:9pt;width:200;height:80;text-align:right;line-height:180%}
span.control			{cursor:hand}
a.mouse					{border-width:0;}
a.mouse:hover			{border:1 inset yellow;cursor:hand;filter:glow(color=green,strength=10);}
a.toolList				{color:#BEF73C;font-size:10pt;text-decoration:underline}
a.toolList:hover		{color:white;font-size:10pt;text-decoration:overline}